@page
@model EnterpriseCarDealership.Pages.CRUDCarBooking.IndexCarBookingModel
@{
}
<table>
    <tbody>
        @if (Model.carBookings.Count == 0)
        {
            <p>Empty list</p>
        }
        else
        {
            foreach (var s in Model.carBookings)
            {
                <tr>
                    <td>@s.Id</td>
                    <td>@s.StartTime</td>
                    <td>@s.EndTime</td>
                    <td>@s.KundeId</td>
                    <td>@s.CarId</td>
                    
                    <td><form asp-page-handler="Delete" asp-route-id="@s.Id"><button type="submit" class="btn btn-primary">Delete Car Booking </button></form></td>
                    <td><a asp-page="UpdateCarBooking" asp-route-id="@s.Id">Update Car Booking </a></td>
                </tr>
            }
        }
    </tbody>
</table>